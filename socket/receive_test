#include <vector>
#include <fstream>
using namespace std;

int main(){
    ifstream in("test.txt");
    string inputStr;
    vector inputContent;
    while(getline(in, inputStr)){
        inputContent.push_back(inputStr);
    }
    in.close();

    ofstream out("output.txt");
    for(int i=0; i < inputContent.size(); i++){
        out << inputContent[i] << endl;
    }
    out.close();

    return 0;
